  "devDependencies": {
    "codecov": ">1.0.0",
    "concat-stream": "^1.5.1",
    "eslint": "^2.2.0",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "qs": "^6.1.0",
    "wav": "^1.0.0"
  },
  "dependencies": {
    "cookie": "~0.2.3",
    "csv-stringify": "~1.0.2",
    "extend": "~3.0.0",
